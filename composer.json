    "keywords":[
      "mysql",
      "laravel",
      "database",
      "cluster",
      "failover"
    ],
    "authors": [
        {
            "name": "Mochamad Gufron",
            "email": "mgufronefendi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "lucor/laravel-auth-file-driver": "0.9.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/commands"
        ],
        "psr-0": {
